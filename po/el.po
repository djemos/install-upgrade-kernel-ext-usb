#: src/install-upgrade-kernel-ext-usb:65
#, sh-format
msgid ""
"USAGE: install-upgrade-kernel-ext-usb [install|upgrade] [huge|generic] [10] "
"[ext4] [/dev/sdb1]"
msgstr ""
"USAGE: install-upgrade-kernel-ext-usb [install|upgrade] [huge|generic] [10] "
"[ext4] [/dev/sdb1]"

#: src/install-upgrade-kernel-ext-usb:67
#, sh-format
msgid "   OPTIONS:"
msgstr "   ΕΠΙΛΟΓΕΣ:"

#: src/install-upgrade-kernel-ext-usb:68
#, sh-format
msgid "   install     Install huge or generic kernel"
msgstr "   install  Εγκατάσταση huge ή generic πυρήνα"

#: src/install-upgrade-kernel-ext-usb:69
#, sh-format
msgid "   upgrade     Upgrade huge or generic kernel removing the old one"
msgstr "   upgrade  Αναβάθμιση huge ή generic πυρήνα απεγκαθιστώντας τον παλιό"

#: src/install-upgrade-kernel-ext-usb:70
#, sh-format
msgid "   huge        Download huge kernel and also create an initrd"
msgstr "   huge     Λήψη huge πυρήνα και επίσης δημιούργησε το initrd"

#: src/install-upgrade-kernel-ext-usb:71
#, sh-format
msgid "   generic     Download generic kernel and also create an initrd"
msgstr "   generic  Λήψη generic πυρήνα και επίσης δημιούργησε το initrd"

#: src/install-upgrade-kernel-ext-usb:72
#, sh-format
msgid "   10       Time in seconds to wait for usb to add to the initrd"
msgstr ""
"   10       Χρόνος σε δευτερ. να περιμένει για το usb να προστεθεί στο initrd"

#: src/install-upgrade-kernel-ext-usb:73
#, sh-format
msgid "   ext4      Filesystem of installed system, default ext4"
msgstr "   ext4     Τύπος αρχείων εγκατεστημένου συστήματος, εξ' ορισμού ext4"

#: src/install-upgrade-kernel-ext-usb:74
#, sh-format
msgid "   /dev/sdb1   Partition, default /dev/sdb1"
msgstr "   /dev/sdb1   Διαμέρισμα, εξ' ορισμού /dev/sdb1"

#: src/install-upgrade-kernel-ext-usb:87
#, sh-format
msgid "ERROR: Too many arguments"
msgstr "ΛΑΘΟΣ: Πάρα πολλές παράμετροι"

#: install-upgrade-kernel-ext-usb.desktop.in.h:1
msgid "install upgrade kernel-ext-usb"
msgstr "εγκατάσταση ενημέρωση πυρήνα σε εξωτερικό USB δίσκο"

#, sh-format
#~ msgid "   huge    \tDownload huge kernel"
#~ msgstr "   huge     Λήψη huge πυρήνα και επίσης δημιούργησε το initrd"
